{
    "name": "servd/craft-svg-field",
    "description": "A Craft CMS plugin for storing SVG content in custom fields",
    "type": "craft-plugin",
    "version": "1.0.0",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "svg",
        "field"
    ],
    "support": {
        "docs": "https://github.com/servdhost/svg-field/blob/master/README.md",
        "issues": "https://github.com/servdhost/svg-field/issues"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Servd",
            "homepage": "https://servd.host"
        }
    ],
    "require": {
        "craftcms/cms": "^5.0.0"
    },
    "autoload": {
        "psr-4": {
            "servd\\svgfield\\": "src/"
        }
    },
    "extra": {
        "name": "SVG Field",
        "handle": "svg-field",
        "class": "servd\\svgfield\\Plugin",
        "basePath": "src/",
        "hasCpSettings": false,
        "hasCpSection": false,
        "changelogUrl": "https://raw.githubusercontent.com/servdhost/svg-field/master/CHANGELOG.md",
        "components": {
            "svgField": "servd\\svgfield\\fields\\SvgField"
        }
    }
}